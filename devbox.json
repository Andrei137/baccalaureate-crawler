{
  "packages": [
    "uv@0.9.21"
  ],
  "shell": {
    "scripts": {
      "parse": [
        "uv run src/main.py"
      ],
      "scrape": [
        "uv run -m src.scraper.main"
      ],
      "check": [
        "find data -mindepth 3 -maxdepth 3 -type d ! -exec test -f \"{}/subiect.pdf\" \\; -print",
        "find data -mindepth 3 -maxdepth 3 -type d ! -exec test -f \"{}/barem.pdf\" \\; -print"
      ],
      "clear": [
        "find data -type f -name \"result.json\" -delete"
      ],
      "count:total": [
        "find . -type f -iname \"subiect.pdf\" | wc -l"
      ],
      "count": [
        "for d in data/*/; do printf \"%s: %d\\n\" \"$(basename \"$d\")\" \"$(find \"$d\" -type f -iname \"subiect.pdf\" | wc -l)\"; done"
      ],
      "finish": [
        "mkdir -p final && for folder in data/*/; do fname=$(basename \"$folder\"); cp \"$folder/result.json\" \"final/$fname.json\"; done"
      ]
    }
  }
}
